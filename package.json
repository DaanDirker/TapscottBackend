{
  "name": "BackendTapscott",
  "version": "1.0.0",
  "description": "Bacis NodeJS Express + Web3 Backend to commincate with our Smart Contract",
  "main": "server.js",
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tunnel": "lt --port 3000 --subdomain tapscott",
    "server": "node server.js",
    "compile": "truffle compile",
    "ganache": "genache-cli",
    "node": "geth --testnet --syncmode 'fast' --rpc --rpcapi db,eth,net,web3,personal --cache=1024  --rpcport 8545 --rpcaddr 127.0.0.1 --rpccorsdomain '*'"
  },
  "keywords": [
    "Ethereum",
    "Web3",
    "Tapscott",
    "NodeJS",
    "Express"
  ],
  "author": "Daan Dirker",
  "license": "ISC",
  "dependencies": {
    "@mollie/api-client": "^3.0.0-beta.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ganache-core": "^2.8.0",
    "nodemon": "^2.0.2",
    "web3": "^1.2.2"
  }
}
